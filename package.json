{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:extension": "npm-watch",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:extension": "next build && next export && npm run lint:build",
    "lint:build": "mv out/_next out/assets && sed -i 's/\\/_next/\\/assets/g' out/**.html",
    "package": "npm run build:extension && zip -r packages/$(date -u +\"%Y-%m-%dT%H:%M:%SZ\").zip out"
  },
  "dependencies": {
    "next": "13.4.13",
    "node-watch": "^0.7.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/react": "18.2.20",
    "autoprefixer": "^10.4.15",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.13",
    "postcss": "^8.4.28",
    "tailwindcss": "^3.3.3",
    "typescript": "5.1.6"
  }
}
